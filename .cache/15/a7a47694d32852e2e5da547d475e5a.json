tion","generated":{"line":102,"column":22},"source":"components/Block.js","original":{"line":48,"column":24}},{"generated":{"line":103,"column":0},"source":"components/Block.js","original":{"line":45,"column":8}},{"generated":{"line":103,"column":21},"source":"components/Block.js","original":{"line":43,"column":6}},{"generated":{"line":103,"column":22},"source":"components/Block.js","original":{"line":42,"column":4}},{"generated":{"line":104,"column":0},"source":"components/Block.js","original":{"line":54,"column":3}},{"name":"Component","generated":{"line":108,"column":2},"source":"components/Block.js","original":{"line":4,"column":20}},{"name":"Block","generated":{"line":110,"column":15},"source":"components/Block.js","original":{"line":74,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\nimport { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\nclass Block extends Component {\n  state = { displayTransaction: false };\n\n  toggleTransaction = () => {\n    this.setState({ displayTransaction: !this.state.displayTransaction });\n  };\n\n  get displayTransaction() {\n    const { data } = this.props.block;\n    // JSON.stringify: Use object as string\n    const stringifiedData = JSON.stringify(data);\n\n    const dataDisplay =\n      stringifiedData.length > 35\n        ? `${stringifiedData.substring(0, 35)}...`\n        : stringifiedData;\n\n    if (this.state.displayTransaction) {\n      return (\n        <div>\n          {data.map(transaction => (\n            <div key={transaction.id}>\n              <hr />\n              <Transaction transaction={transaction} />\n            </div>\n          ))}\n          <br />\n          <Button\n            bsStyle='danger'\n            bsSize='small'\n            onClick={this.toggleTransaction}\n          >\n            Show Less\n          </Button>\n        </div>\n      );\n    }\n\n    return (\n      <div>\n        <div> Data: {dataDisplay} </div>\n        <Button\n          bsStyle='danger'\n          bsSize='small'\n          onClick={this.toggleTransaction}\n        >\n          Show More\n        </Button>\n      </div>\n    );\n  }\n\n  render() {\n    console.log('this.displayTransaction', this.displayTransaction);\n    const { timestamp, hash, lastHash } = this.props.block;\n\n    const hashDisplay = `${hash.substring(0, 15)}...`;\n    const lastHashDisplay = `${lastHash.substring(0, 15)}...`;\n\n    return (\n      <div className='Block'>\n        <div>Hash: {hashDisplay}</div>\n        <div>Last hash: {lastHashDisplay}</div>\n        <div>Timestamp: {new Date(timestamp).toLocaleString()}</div>\n        {this.displayTransaction}\n      </div>\n    );\n  }\n}\n\nexport default Block;\n"},"lineCount":null}},"error":null,"hash":"db24afca2a58d702947142c7b2633937","cacheData":{"env":{}}}